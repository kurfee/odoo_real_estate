<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_order_line_quot_estate" model="ir.ui.view">
        <field name="name">sale.order.line.quot.estate</field>
        <field name="inherit_id" ref="sale.view_order_line_tree"/>
        <field name="model">sale.order.line</field>
        <field name="arch" type="xml">
            <!-- Example of adding fields after a specific field -->
            <xpath expr="//field[@name='price_subtotal']" position="before">
                <field name="last_quotation_price"/>
                <field name="last_quotation_date"/>
            </xpath>
        </field>
    </record>
</odoo>
